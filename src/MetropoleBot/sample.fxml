<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MetropoleBot.Controller">
   <children>
      <GridPane prefHeight="407.0" prefWidth="685.0">
        <columnConstraints>
            <ColumnConstraints maxWidth="255.0" minWidth="10.0" prefWidth="214.0" />
          <ColumnConstraints maxWidth="329.0" minWidth="10.0" prefWidth="224.0" />
          <ColumnConstraints maxWidth="202.0" minWidth="10.0" prefWidth="202.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="start" mnemonicParsing="false" onAction="#start" text="Start" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER" />
            <TextArea fx:id="log" editable="false" prefHeight="308.0" prefWidth="640.0" GridPane.columnSpan="3" GridPane.rowIndex="1" GridPane.rowSpan="2" />
         </children>
      </GridPane>
   </children>
</VBox>
